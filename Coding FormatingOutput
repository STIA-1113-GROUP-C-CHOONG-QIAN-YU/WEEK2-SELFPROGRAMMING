package selfProgramming;

public class FormatingOutput {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
int x=10; double y= 5.373123;
System.out.printf("value of x is %d and value of y is %6.2f%n", x, y);
System.out.printf("%-10s","Total:");
System.out.printf("%-10s",y);
System.out.println("");
System.out.printf("%-10s%10.2f","Total:",y);
	}

}
